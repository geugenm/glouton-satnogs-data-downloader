          {
            "in": "query",
            "name": "vetted_user",
            "schema": {
              "type": "integer"
            },
            "description": "Vetted user (deprecated: will be removed in next version)"
          },
          {
            "in": "query",
            "name": "waterfall_status",
            "schema": {
              "type": "boolean",
              "nullable": true,
              "enum": [
                0,
                1
